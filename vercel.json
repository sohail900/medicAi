{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/old-path",
      "dest": "/"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
